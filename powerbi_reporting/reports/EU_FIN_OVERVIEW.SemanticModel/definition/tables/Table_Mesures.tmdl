	measure 'Overview Subtitle' = ```
			
			VAR MinDate =
			    MIN ( FactInstrumentSnapshot[SnapshotDate] )
			VAR MaxDate =
			    MAX ( FactInstrumentSnapshot[SnapshotDate] )
			VAR NbRows =
			    COUNTROWS ( FactInstrumentSnapshot )
			RETURN
			"Period: "
			    & FORMAT ( MinDate, "yyyy-MM-dd" )
			    & " → "
			    & FORMAT ( MaxDate, "yyyy-MM-dd" )
