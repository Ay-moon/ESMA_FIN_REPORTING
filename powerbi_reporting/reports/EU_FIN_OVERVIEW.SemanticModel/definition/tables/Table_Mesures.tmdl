	measure '% Listings Top10 Venues' = ```
			
			VAR t =
			    TOPN(
			        10,
			        SUMMARIZE(
			            DimTradingVenue,
			            DimTradingVenue[TradingVenueSK],
			            "x", [Listings]
			        ),
			        [x], DESC
			    )
			VAR top10 = SUMX(t, [x])
			RETURN DIVIDE( top10, [Listings] )
			
			```
		lineageTag: f2a333bf-aed9-4697-9094-9ac886b48f38

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure '% Instruments Top10 Issuers' = ```
			
			VAR TotalInstr = sum(Agg_Issuer_Instruments[InstrumentsCnt])
			VAR Top10Sum =
			    SUMX(
			        TOPN(
			            10,
			           Agg_Issuer_Instruments,
			            'Agg_Issuer_Instruments'[InstrumentsCnt],
			            DESC
			        ),
			        'Agg_Issuer_Instruments'[InstrumentsCnt]
			    )
			RETURN
			    DIVIDE( Top10Sum, TotalInstr )
			
			```
		lineageTag: d7289e54-29ac-4f0d-927c-5df5c5079267

